    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "4.18.2",
    "faucet": "0.0.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.5.1",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.2",
    "react-scripts": "^1.0.17",
    "react-test-utils": "0.0.1",
